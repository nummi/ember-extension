{{ui/toolbar-content
  items=(array
    (component "ui/toolbar-icon-button"
      active=inspectingViews
      class="js-inspect-views"
      onclick=(action "toggleViewInspection")
      svgName="toolbar-inspect"
      title="Inspect"
    )
    (component "ui/toolbar-divider")
    (component "ui/toolbar-icon-button"
      onclick=(action "expandOrCollapseAll" true)
      svgName="toolbar-chevron-down"
      title="expand all"
    )
    (component "ui/toolbar-icon-button"
      onclick=(action "expandOrCollapseAll" false)
      svgName="toolbar-chevron-right"
      title="collapse all"
    )
    (component "ui/toolbar-divider")
    (component "ui/toolbar-search-field"
      class="js-filter-views"
      value=searchValue
    )
  )
}}
