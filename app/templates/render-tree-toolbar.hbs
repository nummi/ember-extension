{{#ui/toolbar-content as |bar|}}
  {{ui/toolbar-icon-button
    onclick=(action "clear")
    svgName="toolbar-clear-2"
    title="Clear"
  }}

  {{#unless showEmpty}}
    {{bar.divider}}

    {{ui/toolbar-icon-button
      classNames="js-toolbar-page-refresh-btn"
      onclick=(action "refresh")
      svgName="toolbar-reload"
      title="Reload"
    }}
  {{/unless}}

  {{bar.divider}}

  {{bar.searchField
    class="js-render-profiles-search"
    value=searchValue
  }}
{{/ui/toolbar-content}}
